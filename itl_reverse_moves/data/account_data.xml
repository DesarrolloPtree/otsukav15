<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <record id="sequence_financial_invoice_refund" model="ir.sequence">
            <field name="name">Financial customer invoices refund sequence</field>
            <field name="code">fiancial.account.move.refund</field>
            <field name="prefix">FINV/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

        <record id="sequence_inventory_invoice_refund" model="ir.sequence">
            <field name="name">Inventory customer invoices refund sequence</field>
            <field name="code">inventory.account.move.refund</field>
            <field name="prefix">DINV/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        
    </data>
</odoo>